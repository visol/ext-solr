{
	"name": "apache-solr-for-typo3/solr",
	"type": "typo3-cms-extension",
	"description": "Apache Solr integration for TYPO3",
	"version": "3.0.3-dev",
	"minimum-stability": "alpha",
	"license": "GPL-2.0+",
	"keywords": ["typo3", "TYPO3 CMS", "solr", "search"],
